class test:

	import Tkinter

	def main():
		__init__()

		Accelerate.pack()
		top.mainloop()

		while True:
				snelheid = Tkinter.Text(top)


	def __init__(self):
		top = Tkinter.Tk()
		top.maxsize(400,400)
		Snelheid = 0
		Accelerate = Tkinter.Button(top,text="Accelerate", command = Versnel(Snelheid))

	#functions
	def Versnel(Snelheid):
		versnellen = 25
		Snelheid = Snelheid + versnellen
		print(Snelheid)
